{
    "name": "iqra-islamic-learning",
    "version": "1.0.0",
    "description": "Iqra - Modern Islamic Learning Platform with AI assistance and live classes",
    "type": "module",
    "license": "MIT",
    "keywords": ["islamic", "education", "learning", "quran", "hadith", "ai", "online-classes"],
    "repository": {
      "type": "git",
      "url": "https://github.com/yourusername/iqra-learning.git"
    },
    "scripts": {
      "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
      "dev:server": "NODE_ENV=development tsx watch server/index.ts",
      "dev:client": "vite",
      "build": "npm run build:client && npm run build:server",
      "build:client": "vite build",
      "build:server": "tsc && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/server",
      "start": "NODE_ENV=production node dist/server/index.js",
      "check": "tsc --noEmit",
      "lint": "eslint . --ext .ts,.tsx --fix",
      "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
      "db:generate": "drizzle-kit generate",
      "db:migrate": "drizzle-kit migrate",
      "db:push": "drizzle-kit push",
      "db:studio": "drizzle-kit studio",
      "vercel-build": "npm run build",
      "postinstall": "prisma generate || true"
    },
